                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_23"
                android:layout_below="@+id/ll_center_fensi"
                android:layout_marginRight="@dimen/dp_29"
                android:layout_marginLeft="@dimen/dp_29"
                android:scaleType="fitXY"
                android:adjustViewBounds="true"
                android:background="@drawable/bg_up_grade_free_up"/>
        </RelativeLayout>

        <!--活动商品专区-->
        <LinearLayout
            android:id="@+id/ll_huodongshangpin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rl_center"
            android:layout_marginTop="@dimen/dp_24"
            android:orientation="vertical">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/ic_biaoti_one"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_8"
                android:gravity="center"
                android:text="以下礼包任选一套购买，直升淘觅觅特约店主，尊享特约店主全部权益"
                android:textSize="@dimen/dp_10"
                android:textColor="@color/c_74645A"/>
            <com.zhouwei.mzbanner.MZBannerView
                android:id="@+id/banner_product"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_193"
                android:layout_marginTop="@dimen/dp_14"
                app:canLoop="true"
                app:open_mz_mode="true"
                app:middle_page_cover="false"
                app:open_mz_mode_scaley="false"
                app:open_mz_mode_ex="false"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="以上礼包任选一套购买即可"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_gravity="center"
                android:textColor="@color/c_74645A"
                android:textSize="@dimen/dp_10"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal">
                <TextView
                    android:id="@+id/tv_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_14"
                    android:text="价值 "
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_10"/>
                <TextView
                    android:id="@+id/tv_two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_14"
                    android:layout_toRightOf="@+id/tv_one"
                    android:text="￥"
                    android:textColor="@color/c_FF3333"
                    android:textSize="@dimen/dp_10"/>
                <TextView
                    android:id="@+id/tv_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/tv_two"
                    android:text=""
                    android:textColor="@color/c_FF3333"
                    android:textSize="@dimen/dp_24"/>
            </RelativeLayout>
            <ImageView
                android:id="@+id/iv_up_grade_buy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_19"
                android:layout_marginRight="@dimen/dp_44"
                android:layout_marginLeft="@dimen/dp_44"
                android:scaleType="fitXY"
                android:adjustViewBounds="true"
                android:background="@drawable/bg_up_grade_buy"/>

        </LinearLayout>
        <!--特约店主-->
        <LinearLayout
