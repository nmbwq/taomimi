                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_below="@+id/tv_center_title">
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="直邀粉丝"
                        android:textSize="@dimen/dp_13"
                        android:layout_gravity="center_horizontal"
                        android:textColor="@color/c_74645A"/>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_gravity="center">
                        <com.common.app.view.CircularProgressView
                            android:id="@+id/progress_bar"
                            android:layout_centerInParent="true"
                            android:layout_centerHorizontal="true"
                            android:layout_width="@dimen/dp_67"
                            android:layout_height="@dimen/dp_67"
                            app:inCircleColor="@color/c_E1E1E6"
                            app:outCircleColor="@color/c_E1E1E6"
                            app:progressColor="@color/c_D6A17A"
                            app:lineWidth="@dimen/dp_1" />
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/tv_zhiyao_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="50"
                                android:textColor="@color/c_D6A17A"
                                android:textSize="@dimen/dp_19"/>
                            <TextView
                                android:id="@+id/tv_zhiyao_need_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="/60"/>
                        </LinearLayout>

                    </RelativeLayout>

                    <TextView
                        android:id="@+id/tv_need_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_14"
                        android:text="共需要邀请60人"
                        android:textSize="@dimen/dp_13"
                        android:layout_gravity="center_horizontal"
                        android:textColor="@color/c_74645A"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="间接粉丝"
                        android:textSize="@dimen/dp_13"
                        android:layout_gravity="center_horizontal"
                        android:textColor="@color/c_74645A"/>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_gravity="center">
                        <com.common.app.view.CircularProgressView
                            android:id="@+id/progress_bar_other"
                            android:layout_centerInParent="true"
                            android:layout_centerHorizontal="true"
                            android:layout_width="@dimen/dp_67"
                            android:layout_height="@dimen/dp_67"
                            app:inCircleColor="@color/c_E1E1E6"
                            app:outCircleColor="@color/c_E1E1E6"
                            app:progressColor="@color/c_D6A17A"
                            app:lineWidth="@dimen/dp_1" />
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/tv_zhiyao_other_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="50"
                                android:textColor="@color/c_D6A17A"
                                android:textSize="@dimen/dp_19"/>
                            <TextView
                                android:id="@+id/tv_zhiyao_other_need_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="/60"/>
                        </LinearLayout>

                    </RelativeLayout>

                    <TextView
