                android:text="特约店主权益详情"
                android:textSize="@dimen/dp_13"
                android:textColor="@color/c_2D2D32"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_11"
                android:gravity="center"
                android:visibility="gone"
                >
                <android.support.v7.widget.CardView
                    android:id="@+id/cv_item_four"
                    android:layout_width="@dimen/dp_38"
                    android:layout_height="@dimen/dp_38"
                    android:layout_centerVertical="true"
                    app:cardCornerRadius="@dimen/dp_16"
                    app:cardElevation="@dimen/dp_0"
                    app:cardPreventCornerOverlap="false"
                    >
                    <ImageView
                        android:id="@+id/iv_user_photo_four"
                        android:layout_width="@dimen/dp_38"
                        android:layout_height="@dimen/dp_38"/>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/cv_item_five"
                    android:layout_width="@dimen/dp_38"
                    android:layout_height="@dimen/dp_38"
                    android:layout_marginLeft="@dimen/dp_8"
                    android:layout_toRightOf="@+id/cv_item_four"
                    android:layout_centerVertical="true"
                    app:cardCornerRadius="@dimen/dp_16"
                    app:cardElevation="@dimen/dp_0"
                    app:cardPreventCornerOverlap="false"
                    >
                    <ImageView
                        android:id="@+id/iv_user_photo_five"
                        android:layout_width="@dimen/dp_38"
                        android:layout_height="@dimen/dp_38" />
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/cv_item_one"
                    android:layout_width="@dimen/dp_58"
                    android:layout_height="@dimen/dp_58"
                    android:layout_marginLeft="@dimen/dp_8"
                    android:layout_toRightOf="@+id/cv_item_five"
                    android:layout_centerVertical="true"
                    app:cardCornerRadius="@dimen/dp_16"
                    app:cardElevation="@dimen/dp_0"
                    app:cardPreventCornerOverlap="false"
                    >
                    <ImageView
                        android:id="@+id/iv_user_photo_one"
                        android:layout_width="@dimen/dp_58"
                        android:layout_height="@dimen/dp_58"
                        />
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/cv_item_two"
                    android:layout_width="@dimen/dp_38"
                    android:layout_height="@dimen/dp_38"
                    android:layout_marginLeft="@dimen/dp_8"
                    android:layout_toRightOf="@+id/cv_item_one"
                    android:layout_centerVertical="true"
                    app:cardCornerRadius="@dimen/dp_16"
                    app:cardElevation="@dimen/dp_0"
                    app:cardPreventCornerOverlap="false"
                    >
                    <ImageView
                        android:id="@+id/iv_user_photo_two"
                        android:layout_width="@dimen/dp_38"
                        android:layout_height="@dimen/dp_38"
                        />
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/cv_item_three"
                    android:layout_width="@dimen/dp_38"
                    android:layout_height="@dimen/dp_38"
                    android:layout_marginLeft="@dimen/dp_8"
                    android:layout_toRightOf="@+id/cv_item_two"
                    android:layout_centerVertical="true"
                    app:cardCornerRadius="@dimen/dp_16"
                    app:cardElevation="@dimen/dp_0"
                    app:cardPreventCornerOverlap="false"
                    >
                    <ImageView
                        android:id="@+id/iv_user_photo_three"
                        android:layout_width="@dimen/dp_38"
                        android:layout_height="@dimen/dp_38"
                        />
                </android.support.v7.widget.CardView>
            </RelativeLayout>

            <android.support.v4.view.ViewPager
                android:id="@+id/viewPager"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:clipChildren="false"
                android:layout_marginLeft="@dimen/dp_30"
                android:layout_marginRight="@dimen/dp_30"
                />

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/c_74645A"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_47"
                android:layout_marginRight="@dimen/dp_47"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_marginTop="13dp"
                android:orientation="vertical"
                android:background="@drawable/bg_up_grade_content">
                <TextView
                    android:id="@+id/tv_money"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_22"
                    android:layout_gravity="center"
                    android:text="5214"
                    android:textColor="@color/c_DDAC68"
                    android:textSize="@dimen/dp_20"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_8"
                    android:text="上月收入"
                    android:textSize="@dimen/dp_12"
                    android:layout_gravity="center"
                    android:textColor="@color/c_74645A"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_6"
                    android:layout_marginBottom="@dimen/dp_12"
                    android:text="*以上信息均为获得用户同意公开"
                    android:textSize="@dimen/dp_12"
                    android:layout_gravity="center"
                    android:textColor="@color/c_949292"/>


            </LinearLayout>
        </LinearLayout>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_below="@+id/ll_teyuedianzhu"
            android:layout_centerHorizontal="true"
            android:background="@drawable/ic_up_grade_black_fang"/>
    </RelativeLayout>
