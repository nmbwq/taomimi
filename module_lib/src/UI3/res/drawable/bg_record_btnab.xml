    <corners
        android:bottomLeftRadius="@dimen/dp_20"
        android:bottomRightRadius="@dimen/dp_20"
        android:topLeftRadius="@dimen/dp_20"
        android:topRightRadius="@dimen/dp_20" />
</shape>