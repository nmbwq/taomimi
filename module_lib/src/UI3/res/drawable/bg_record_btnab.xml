    <solid android:color="@color/white" />
    <!-- 边框色 -->
    <stroke
        android:width="0.01dp"
        android:color="@color/c_ABABAB" />
    <padding
        android:bottom="1dp"
        android:left="0.5dp"
        android:right="0.5dp"
        android:top="0dp" />
