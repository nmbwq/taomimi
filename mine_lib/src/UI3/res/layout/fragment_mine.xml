<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <include
        layout="@layout/common_web_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        />
    <RelativeLayout
        android:id="@+id/up_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="gone">

    </RelativeLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_mine_all"
            />

        <ImageView
            android:id="@+id/iv_news"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginTop="@dimen/dp_25"
            android:padding="@dimen/dp_10"
            android:src="@drawable/ic_mine_news"
            />

        <com.scwang.smartrefresh.layout.SmartRefreshLayout
            android:id="@+id/smart_earnings"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp_65"
            app:srlDragRate="0.5"
            app:srlEnableAutoLoadMore="true"
            app:srlFooterHeight="@dimen/dp_25"
            app:srlFooterTriggerRate="1"
            app:srlHeaderHeight="@dimen/dp_40"
            app:srlHeaderMaxDragRate="1.5"
            app:srlHeaderTriggerRate="1"
            app:srlReboundDuration="200"
            >

            <com.scwang.smartrefresh.layout.header.ClassicsHeader
                android:id="@+id/smart_earnings_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srlDrawableArrowSize="@dimen/sp_14"
                app:srlEnableLastTime="true"
                app:srlTextSizeTitle="@dimen/sp_14"
                app:srlPrimaryColor="@color/transparent"
                app:srlAccentColor="@color/white"
                />

            <android.support.v4.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/up_1"
                android:fillViewport="true"
                >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >

                    <RelativeLayout
                        android:id="@+id/ll_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clipChildren="false"
                        >

                        <RelativeLayout
                            android:id="@+id/rl_user"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_mine_up"
                            />

                        <ImageView
                            android:id="@+id/iv_photo"
                            android:layout_width="@dimen/dp_60"
                            android:layout_height="@dimen/dp_60"
                            android:layout_marginLeft="@dimen/dp_14"
                            android:layout_marginTop="@dimen/dp_15"
                            android:scaleType="centerInside"
                            android:src="@drawable/ic_common_user_def"
                            />

                        <TextView
                            android:id="@+id/tv_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_8"
                            android:layout_marginTop="@dimen/dp_23"
                            android:layout_toRightOf="@+id/iv_photo"
                            android:text="用户昵称"
                            android:textColor="@color/white"
                            android:textSize="@dimen/dp_12"
                            android:textStyle="bold"
                            />

                        <TextView
                            android:id="@+id/tv_fensi"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/tv_name"
                            android:layout_marginLeft="@dimen/dp_8"
                            android:layout_marginTop="@dimen/dp_1"
                            android:layout_toRightOf="@+id/iv_photo"
                            android:text="粉丝数"
                            android:textColor="@color/white"
                            android:textSize="@dimen/dp_12"
                            android:visibility="invisible"
                            />

                        <ImageView
                            android:id="@+id/iv_mine_huiyuan"
                            android:layout_width="@dimen/dp_53"
                            android:layout_height="@dimen/dp_13"
                            android:layout_below="@+id/tv_fensi"
                            android:layout_marginLeft="@dimen/dp_8"
                            android:layout_marginTop="@dimen/dp_2"
                            android:layout_toRightOf="@+id/iv_photo"
                            />

                        <RelativeLayout
                            android:id="@+id/ll_up1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_30"
                            >

                            <LinearLayout
                                android:id="@+id/ll_yaoqingma_parent"
                                android:layout_width="@dimen/dp_86"
                                android:layout_height="@dimen/dp_30"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="@dimen/dp_13"
                                android:background="@drawable/bg_mine_tv_fuzhi"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:id="@+id/tv_InviteCode"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:text=""
                                    android:textAllCaps="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/dp_12"
                                    android:textStyle="bold"
                                    />

                                <TextView
                                    android:id="@+id/tv_InviteCodeCopy"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:text="复制激活码"
                                    android:textAllCaps="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/dp_10"
                                    />

                            </LinearLayout>

                        </RelativeLayout>

                        <!--顶部内容-->
                        <RelativeLayout
                            android:id="@+id/up_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/rl_user"
                            android:paddingLeft="@dimen/dp_12"
                            android:paddingRight="@dimen/dp_12"
                            android:background="@color/white"
                            >

                            <TextView
                                android:id="@+id/tv_money"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="¥0"
                                android:textAllCaps="true"
                                android:textColor="@color/c_FE3F47"
                                android:textSize="@dimen/dp_16"
                                android:textStyle="bold"
                                />

                            <TextView
                                android:id="@+id/tv_zi1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/tv_money"
                                android:text="可提现金额(元)"
                                android:textSize="@dimen/dp_12"
                                android:textColor="@color/c_2F2F2F"
                                />

                            <TextView
                                android:id="@+id/tv_withdrawal"
                                android:layout_width="@dimen/dp_51"
                                android:layout_height="@dimen/dp_19"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentTop="true"
                                android:layout_marginTop="@dimen/dp_7"
                                android:gravity="center"
                                android:background="@drawable/bg_mine_tv_tixian"
                                android:text="提现"
                                android:textColor="@color/c_FE3F47"
                                android:textSize="@dimen/dp_12"
                                />

                            <View
                                android:id="@+id/v_fengexian1"
                                android:layout_width="match_parent"
                                android:layout_height="1px"
                                android:layout_below="@+id/tv_zi1"
                                android:layout_marginTop="@dimen/dp_13"
                                android:background="@color/c_D9D9D9"
                                />


                            <!--顶部的底部部分-->
                            <LinearLayout
                                android:id="@+id/ll_tiaomu"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/v_fengexian1"
                                android:layout_marginBottom="@dimen/dp_13"
                                android:layout_marginTop="@dimen/dp_13"
                                android:gravity="center"
                                >

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical"
                                    >

                                    <TextView
                                        android:id="@+id/tv_earnings"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="¥0"
                                        android:textAllCaps="true"
                                        android:textColor="@color/c_2F2F2F"
                                        android:textSize="@dimen/sp_11"
                                        android:textStyle="bold"
                                        />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="今日收益"
                                        android:textAllCaps="true"
                                        android:textColor="@color/c_9C9C9C"
                                        android:textSize="@dimen/sp_10"
                                        />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical"
                                    >

                                    <TextView
                                        android:id="@+id/tv_forecast"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="¥0"
                                        android:textAllCaps="true"
                                        android:textColor="@color/c_2F2F2F"
                                        android:textSize="@dimen/sp_11"
                                        android:textStyle="bold"
                                        />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="本月预估"
                                        android:textAllCaps="true"
                                        android:textColor="@color/c_9C9C9C"
                                        android:textSize="@dimen/sp_10"
                                        />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical"
                                    >

                                    <TextView
                                        android:id="@+id/tv_shangyueyugu"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="¥0"
                                        android:textAllCaps="true"
                                        android:textColor="@color/c_2F2F2F"
                                        android:textSize="@dimen/sp_11"
                                        android:textStyle="bold"
                                        />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="上月预估"
                                        android:textAllCaps="true"
                                        android:textColor="@color/c_9C9C9C"
                                        android:textSize="@dimen/sp_10"
                                        />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical"
                                    >

                                    <TextView
                                        android:id="@+id/tv_settlement"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="¥0"
                                        android:textAllCaps="true"
                                        android:textColor="@color/c_2F2F2F"
                                        android:textSize="@dimen/sp_11"
                                        android:textStyle="bold"
                                        />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="上月结算"
                                        android:textAllCaps="true"
                                        android:textColor="@color/c_9C9C9C"
                                        android:textSize="@dimen/sp_10"
                                        />

                                </LinearLayout>

                            </LinearLayout>
                        </RelativeLayout>

                        <com.zhouwei.mzbanner.MZBannerView
                            android:id="@+id/banner_mine_huodong"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_90"
                            android:visibility="gone"
                            app:canLoop="true"
                            app:indicatorAlign="center"
                            app:indicatorPaddingLeft="10dp"
                            app:middle_page_cover="false"
                            app:open_mz_mode="true"
                            app:open_mz_mode_scaley="false"
                            />

                    </RelativeLayout>

                    <View
                        android:id="@+id/v_fengexian2"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_14"
                        android:layout_below="@+id/ll_1"
                        android:background="@color/c_F6F6F6"
                        />

                    <RelativeLayout
                        android:id="@+id/rl_yaoqinghaoyou"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/v_fengexian2"
                        android:background="@color/white"
                        >

                        <TextView
                            android:id="@+id/tv_dingwei1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_marginTop="@dimen/dp_8"
                            android:text="邀请好友"
                            android:textColor="@color/c_2F2F2F"
                            android:textSize="@dimen/dp_12"
                            />

                        <ImageView
                            android:layout_width="@dimen/dp_6"
                            android:layout_height="@dimen/dp_12"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/dp_15"
                            android:layout_marginTop="@dimen/dp_11"
                            android:background="@drawable/ic_mine_zuojiantou"
                            />

                        <View
                            android:id="@+id/v_fengexian3"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_1"
                            android:layout_below="@+id/tv_dingwei1"
                            android:layout_marginTop="@dimen/dp_7"
                            android:background="@color/c_D9D9D9"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_marginRight="@dimen/dp_12"
                            />

                        <ImageView
                            android:id="@+id/iv_zhanweitu"
                            android:layout_width="@dimen/dp_331"
                            android:layout_height="@dimen/dp_72"
                            android:layout_below="@+id/v_fengexian3"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/dp_9"
                            android:background="@drawable/bg_mine_yaoqinghaoyou"
                            />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_14"
                            android:layout_below="@+id/iv_zhanweitu"
                            android:layout_marginTop="@dimen/dp_11"
                            android:background="@color/c_F6F6F6"
                            />

                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/ll"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/rl_yaoqinghaoyou"
                        android:orientation="vertical"
                        android:background="@color/white"
                        >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_marginTop="@dimen/dp_7"
                            android:text="我的服务"
                            android:textColor="@color/c_2F2F2F"
                            android:textSize="@dimen/dp_12"
                            />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_1"
                            android:layout_marginLeft="@dimen/dp_14"
                            android:layout_marginRight="@dimen/dp_14"
                            android:layout_marginTop="@dimen/dp_7"
                            android:background="@color/c_D9D9D9"
                            />

                        <LinearLayout
                            android:id="@+id/ll_mine_mytequantiaomu"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_12"
                            android:orientation="horizontal"
                            >

                            <LinearLayout
                                android:id="@+id/ll_myOrder"
                                android:layout_width="0px"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/dp_2"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                >

                                <ImageView
                                    android:layout_width="@dimen/dp_23"
                                    android:layout_height="@dimen/dp_23"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_mine_order"
                                    />
                                <!--ic_mine_order 我的订单 ll_myOrder-->

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_2"
                                    android:gravity="center_horizontal"
                                    android:text="我的订单"
                                    android:textColor="@color/c_2F2F2F"
                                    android:textSize="@dimen/dp_12"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/ll_myFans"
                                android:layout_width="0px"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_2"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                >

                                <ImageView
                                    android:layout_width="@dimen/dp_23"
                                    android:layout_height="@dimen/dp_23"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_mine_fensi"
                                    />
                                <!--ic_mine_fensi 我的粉丝 ll_myFans-->
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_2"
                                    android:gravity="center_horizontal"
                                    android:text="我的粉丝"
                                    android:textColor="@color/c_181818"
                                    android:textSize="@dimen/dp_12"
                                    />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/ll_myEarnings"
                                android:layout_width="0px"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_2"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:orientation="vertical"
                                >

                                <ImageView
                                    android:layout_width="@dimen/dp_23"
                                    android:layout_height="@dimen/dp_23"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_mine_shouyi"
                                    />
                                <!--ic_mine_shouyi 我的收益 ll_myEarnings-->
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_2"
                                    android:gravity="center_horizontal"
                                    android:text="我的收益"
                                    android:textColor="@color/c_181818"
                                    android:textSize="@dimen/dp_12"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/tv_shouchang"
                                android:layout_width="0px"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/dp_2"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                >

                                <ImageView
                                    android:layout_width="@dimen/dp_23"
                                    android:layout_height="@dimen/dp_23"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_mine_shoucang"
                                    />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_2"
                                    android:gravity="center_horizontal"
                                    android:text="我的收藏"
                                    android:textColor="@color/c_181818"
                                    android:textSize="@dimen/dp_12"
                                    />

                            </LinearLayout>


                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_1"
                            android:layout_marginLeft="@dimen/dp_14"
                            android:layout_marginRight="@dimen/dp_14"
                            android:layout_marginTop="@dimen/dp_12"
                            android:background="@color/c_D9D9D9"
                            />

                        <LinearLayout
                            android:id="@+id/ll_changyong_parent"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_12"
                            android:gravity="center"
                            android:orientation="horizontal"
                            >

                            <LinearLayout
                                android:id="@+id/tv_xinshou"
                                android:layout_width="0px"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/dp_2"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                >

                                <ImageView
                                    android:layout_width="@dimen/dp_23"
                                    android:layout_height="@dimen/dp_23"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_mine_xinshou"
                                    />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_2"
                                    android:gravity="center_horizontal"
                                    android:text="新手指引"
                                    android:textColor="@color/c_181818"
                                    android:textSize="@dimen/dp_12"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/tv_wenti"
                                android:layout_width="0px"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/dp_2"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                >

                                <ImageView
                                    android:layout_width="@dimen/dp_23"
                                    android:layout_height="@dimen/dp_23"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_mine_wenti"
                                    />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_2"
                                    android:gravity="center_horizontal"
                                    android:text="常见问题"
                                    android:textColor="@color/c_181818"
                                    android:textSize="@dimen/dp_12"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/tv_kefu"
                                android:layout_width="0px"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/dp_2"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                >

                                <ImageView
                                    android:layout_width="@dimen/dp_23"
                                    android:layout_height="@dimen/dp_23"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_mine_kefu"
                                    />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_2"
                                    android:gravity="center_horizontal"
                                    android:text="专属客服"
                                    android:textColor="@color/c_181818"
                                    android:textSize="@dimen/dp_12"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/tv_gonggao"
                                android:layout_width="0px"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/dp_2"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                >

                                <ImageView
                                    android:layout_width="@dimen/dp_23"
                                    android:layout_height="@dimen/dp_23"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_mine_gonggao"
                                    />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/dp_2"
                                    android:gravity="center_horizontal"
                                    android:text="官方公告"
                                    android:textColor="@color/c_181818"
                                    android:textSize="@dimen/dp_12"
                                    />

                            </LinearLayout>
                        </LinearLayout>

